{
    "kind": "Pod",
    "apiVersion": "v1beta1",
    "id": "pod-nginx-01",
    "desiredState": {
        "manifest": {
            "version": "v1beta1",
            "containers": [{
                "name": "pod-nginx-01",
                "image": "dockerfile/nginx",
                "ports": [{
                    "containerPort": 80
                }],
                "livenessProbe": {
                    "enabled": true,
                    "type": "http",
                    "initialDelaySeconds": 30,
                    "httpGet": {
                        "path": "/",
                        "port": "80"
                    }
                }
            }]
        }
    },
    "labels": {
        "name": "pod-nginx"
    }
}
